{
  "name": "ngx-simple-form",
  "version": "0.0.129",
  "scripts": {
    "lib:deploy": "npm run lib:build && cd dist/ && npm publish",
    "lib:build": "ng-packagr -p package.json && cd ../ngx-painless-form-test/src/app && echo '' >> app.module.ts && clear"
  },
  "peerDependencies": {
    "@angular/common": "^13.0.0",
    "@angular/core": "^13.0.0",
    "jquery": "^3.5.x",
    "ng-select2": "^1.3.1",
    "select2": "^4.0.x"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "ngPackage": {
    "$schema": "./../node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "cssUrl": "inline",
      "entryFile": "public-api.ts"
    },
    "assets": [
      "assets/**"
    ],
    "dest": "dist"
  }
}
